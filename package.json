{
  "name": "svelte-redux-adapter",
  "version": "1.0.3",
  "author": {
    "name": "Cola Fanta Wlk <colafantawlk@gmail.com>"
  },
  "homepage": "https://github.com/ColaFanta/svelte-redux-adapter",
  "repository": {
    "type": "git",
    "url": "https://github.com/ColaFanta/svelte-redux-adapter"
  },
  "license": "MIT",
  "engines": {
    "pnpm": ">=7"
  },
  "keywords": [
    "svelte",
    "svelte-redux",
    "redux",
    "store"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && pnpm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package -i src/lib/package && publint",
    "prepublishOnly": "pnpm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "svelte": "^3.54.0"
  },
  "devDependencies": {
    "@fontsource/fira-mono": "^4.5.10",
    "@playwright/test": "^1.33.0",
    "@reduxjs/toolkit": "^1.9.5",
    "@sveltejs/adapter-auto": "^2.0.1",
    "@sveltejs/kit": "^1.16.0",
    "@sveltejs/package": "^2.0.2",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/cookie": "^0.5.1",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitest/coverage-c8": "^0.31.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.0",
    "publint": "^0.1.11",
    "svelte": "^3.58.0",
    "svelte-check": "^3.3.0",
    "svelte-preprocess": "^5.0.3",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.4",
    "vitest": "^0.31.0"
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module"
}
